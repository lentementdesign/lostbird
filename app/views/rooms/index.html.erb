<div class="col-sm-12">
<% if @anotherEntries.count == 0 %>
  <p>メッセージがありません。</p>
<% end %>
<% @anotherEntries.each do |e| %>
  <%= link_to room_path(e.room.id) do %>
    <p><%= e.user.nickname %>さん</p>
    <p><%= e.room.messages.last.content %></p>
    <p><%= e.room.messages.last.created_at %></p>
  <% end %>
<% end %>
</div>
